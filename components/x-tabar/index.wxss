.tabbar{
  display: flex;
  height: 50px;
  align-items: center;
  font-size: 14px;
  color: #8a8a8a;
  background-color: white;
  justify-content: space-around;
  box-shadow: 0px -1px 0px 0px #f0f0f0;
}
.tabbar-item{
  position: relative;
  display: flex;
  height: 100%;
  width: 100%;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.circle{
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 60px;
  bottom: 10px;
  height: 60px;
  border-radius: 50%;
  box-shadow: 0px -1px 0px 0px #f0f0f0;
  background-color: white;
}
.active{
  color: #51a7d8;
}
.row{
  flex-direction: row;
}
.col{
  flex-direction: column;
}