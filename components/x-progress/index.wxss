:host{
  width: 100%;
}
.progress{
  position: relative;
  background-color: #e0e0e0;
  border-radius:5px;
}
.progress-item{
  border-radius:5px;
}
.label{
  width: 100%;
  position: absolute;
  left: 0px;
  top: 0px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
}